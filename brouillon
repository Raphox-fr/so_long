		printf("ta mere\n");
	
		mlx_clear_window(game->mlx, game->win);
		mlx_destroy_window(game->mlx, game->win);
		mlx_destroy_display(game->mlx);
		// mlx_loop_end(game->mlx);
		free(game->string);
		free_map(game->map);
		free_map(game->map_to_fill);
		free(game->mlx);
		free(game);
		// ft_close(game);
		exit(1);